#!/bin/bash
# Check common files have no differences

cd ~/JavaDev/Pentad/Arduino/SignalBox/SignalBox

DIR=OutputModule
echo
for FILE in Config.h I2cComms.h Messages.h OutputDef.h Persisted.h System.h
do
    echo $DIR/$FILE
    diff $FILE ../$DIR
done

DIR=Gateway
echo
for FILE in I2cComms.h
do
    echo $DIR/$FILE
    diff $FILE ../$DIR
done
