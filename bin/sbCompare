#!/bin/bash
# Check common files have no differences

cd ~/JavaDev/Pentad/Arduino/SignalBox/SignalBox
for FILE in All.h Config.h I2cComms.h Messages.h Output.h System.h System.ino
do
    echo $FILE
    diff $FILE ../OutputModule
done

for FILE in I2cComms.h
do
    echo $FILE
    diff $FILE ../Gateway
done
